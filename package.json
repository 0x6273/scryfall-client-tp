{
  "name": "scryfall-client",
  "version": "0.13.0",
  "description": "A module for making requests to scryfall",
  "main": "index.js",
  "files": [
    "index.js",
    "lib/*",
    "models/*"
  ],
  "scripts": {
    "build": "webpack",
    "build:watch": "webpack-dev-server",
    "predemo": "npm run build",
    "demo": "http-server docs -p 4567",
    "demo:dev": "npm run demo & npm run build:watch",
    "lint": "eslint --ext ts .",
    "pretty": "prettier --write .",
    "posttest": "npm run lint",
    "test": "jest",
    "test:unit": "jest test/unit/**/*.js",
    "test:integration": "jest test/integration/*.js",
    "test:publishing": "jest test/publishing/*.js",
    "test:all": "npm run lint && npm run test:unit && npm run test:integration && npm run test:publishing"
  },
  "keywords": [
    "mtg",
    "magic the gathering",
    "scryfall"
  ],
  "author": "blade <blade@crookedneighbor.com>",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "check-ecmascript-version-compatibility": "^0.1.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "http-server": "^0.12.1",
    "jest": "^25.4.0",
    "mkdirp": "^1.0.3",
    "prettier": "^2.0.5",
    "ts-jest": "^25.4.0",
    "ts-loader": "^7.0.1",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "superagent": "^5.2.2"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./test/helpers/setup.js"
    ]
  }
}
