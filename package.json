{
  "name": "scryfall-client",
  "version": "0.13.0",
  "description": "A module for making requests to scryfall",
  "main": "index.js",
  "files": [
    "index.js",
    "lib/*",
    "models/*"
  ],
  "scripts": {
    "build": "browserify docs/src/index.js -o docs/script.js",
    "build:watch": "watchify docs/src/index.js -o docs/script.js",
    "predemo": "npm run build",
    "demo": "http-server docs -p 4567",
    "demo:dev": "npm run demo & npm run build:watch",
    "lint": "eslint .",
    "pretty": "prettier --write .",
    "posttest": "npm run lint",
    "test": "npm run test:unit",
    "test:unit": "jest test/unit/**/*.js",
    "test:integration": "jest test/integration/**/*.js",
    "test:publishing": "jest test/publishing/**/*.js",
    "test:all": "npm run lint && npm run test:unit && npm run test:integration && npm run test:publishing"
  },
  "keywords": [
    "mtg",
    "magic the gathering",
    "scryfall"
  ],
  "author": "blade <blade@crookedneighbor.com>",
  "license": "MIT",
  "devDependencies": {
    "browserify": "^16.5.0",
    "check-ecmascript-version-compatibility": "^0.1.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "http-server": "^0.12.1",
    "jest": "^25.4.0",
    "mkdirp": "^1.0.3",
    "prettier": "^2.0.5",
    "watchify": "^3.11.1"
  },
  "dependencies": {
    "superagent": "^5.2.2"
  }
}
